<h1>Stock List</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Symbol</th>
      <th>Price</th>
      <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stocklist.each do |stock| %>
      <tr>
        <td><%= stock.id %></td>
        <td><%= stock.symbol %></td>
        <td><%= stock.price %></td>
        <td><%= stock.info %></td>
        <td><%= link_to 'show', stock %></td>
        <td><%= link_to 'Edit', stock_path(stock) %></td>
      </tr>
    <% end %>
  </tbody>
</table>